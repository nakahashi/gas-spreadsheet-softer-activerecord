    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "ncp": "^2.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^4.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.8.3"
